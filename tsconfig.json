{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es5",
      "dom",
      "es6",
      "es2015.collection",
      "es2015.promise",
      "es2017",
      "es2015",
      "ES2021.String"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "inlineSources": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/Components/SelectSource/PdfDownlaod.jsx"]
}
